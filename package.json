{
  "name": "@thepassle/app-tools",
  "version": "0.9.13",
  "description": "",
  "type": "module",
  "scripts": {
    "test": "wtr **/*.test.js --node-resolve",
    "test:watch": "npm run test -- --watch",
    "lint:types": "tsc",
    "lint:types:watch": "tsc --watch"
  },
  "exports": {
    "./state.js": { 
      "types": "./types/state/index.d.ts",
      "default": "./state.js"
    },
    "./pwa.js": { 
      "types": "./types/pwa/index.d.ts",
      "default": "./pwa.js"
    },
    "./dialog.js": { 
      "types": "./types/dialog/index.d.ts",
      "default": "./dialog.js"
    },
    "./pwa/*": { 
      "types": "./types/pwa/*",
      "default": "./pwa/*"
    },
    "./api.js": { 
      "types": "./types/api/index.d.ts",
      "default": "./api.js"
    },
    "./router.js": { 
      "types": "./types/router/index.d.ts",
      "default": "./router.js"
    },
    "./router/plugins/*": { 
      "types": "./types/router/plugins/*",
      "default": "./router/plugins/*"
    },
    "./api/plugins/*": { 
      "types": "./types/api/plugins/*",
      "default": "./api/plugins/*"
    },
    "./utils.js": { 
      "types": "./types/utils/index.d.ts",
      "default": "./utils.js"
    },
    "./utils/*": { 
      "types": "./types/utils/*",
      "default": "./utils/*"
    },
    "./env.js": {
      "types": "./types/env/dev.js",
      "development": "./env/dev.js",
      "default": "./env/prod.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "./types/**/*.d.ts",
    "./env.js",
    "./pwa.js",
    "./dialog.js",
    "./dialog/*.js",
    "./pwa/*.js",
    "./router.js",
    "./router/index.js",
    "./router/plugins/*.js",
    "./api.js",
    "./api/index.js",
    "./api/plugins/*.js",
    "./state.js",
    "./state/index.js",
    "./utils.js",
    "./utils/*.js",
    "./env/dev.js",
    "./env/prod.js"
  ],
  "keywords": [
    "router",
    "state",
    "pwa",
    "api",
    "fetch",
    "client",
    "abortcontroller"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@open-wc/testing": "^3.1.6",
    "@web/test-runner": "^0.13.31",
    "sinon": "^14.0.0",
    "typescript": "^4.9.4"
  }
}
