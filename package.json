{
  "name": "@thepassle/app-tools",
  "version": "0.7.4",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "wtr **/*.test.js --node-resolve",
    "test:watch": "npm run test -- --watch",
    "lint:types": "tsc",
    "lint:types:watch": "tsc --watch"
  },
  "exports": {
    "./state.js": "./state.js",
    "./pwa.js": "./pwa.js",
    "./dialog.js": {
      "types": "./dialog/types.d.ts",
      "default": "./dialog.js"
    },
    "./pwa/*": "./pwa/*",
    "./api.js": {
      "types": "./api/types.d.ts",
      "default": "./api.js"
    },
    "./router.js": {
      "types": "./router/types.d.ts",
      "default": "./router.js"
    },
    "./router/plugins/*": "./router/plugins/*",
    "./api/plugins/*": "./api/plugins/*",
    "./utils.js": "./utils.js",
    "./utils/*": "./utils/*",
    "./env.js": {
      "development": "./env/dev.js",
      "default": "./env/prod.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "./**/*.d.ts",
    "./env.js",
    "./pwa.js",
    "./dialog.js",
    "./dialog/*.js",
    "./pwa/*.js",
    "./router.js",
    "./router/index.js",
    "./router/plugins/*.js",
    "./api.js",
    "./api/index.js",
    "./api/plugins/*.js",
    "./state.js",
    "./state/index.js",
    "./utils.js",
    "./utils/*.js",
    "./env/env-dev.js",
    "./env/env-prod.js"
  ],
  "keywords": [
    "router",
    "state",
    "pwa",
    "api",
    "fetch",
    "client",
    "abortcontroller"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@open-wc/testing": "^3.1.6",
    "@web/test-runner": "^0.13.31",
    "sinon": "^14.0.0",
    "typescript": "^4.9.4"
  }
}
